{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
				"prettier",
				"plugin:prettier/recommended"
    ],
		"plugins": [
			"prettier"
		],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
			"prettier/prettier": "error"
    }
}
